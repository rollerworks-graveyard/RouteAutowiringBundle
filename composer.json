{
    "name": "rollerworks/route-autowiring-bundle",
    "description": "Easily load (autowire) routes in any part of your routing schema",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok ",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "^2.8.9 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Bundle\\RouteAutowiringBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "test/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Bundle\\RouteAutowiringBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3.6",
        "symfony/yaml": "^2.8.9 || ^3.0",
        "symfony/property-access": "^2.8.9 || ^3.0",
        "symfony/browser-kit": "^2.8.9 || ^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
